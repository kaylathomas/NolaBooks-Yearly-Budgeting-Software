<div id="savingsCollapseDiv">
    <div class="form-row">
        <div class="col">
            <label>Savings(General):</label>
            <div class="input-group mb-3">
                <div class="input-group-prepend">
                    <span class="input-group-text">$</span>
                </div>
                <input type="number" class="form-control" id="monthlySavings">
            </div>
        </div>
        <div class="col">
            <label>Emergency Fund:</label>
            <div class="input-group mb-3">
                <div class="input-group-prepend">
                    <span class="input-group-text">$</span>
                </div>
                <input type="number" class="form-control" id="monthlyEmergency">
            </div>
        </div>
    </div>
    <div class="form-row">
        <div class="col">
            <label>401k:</label>
            <div class="input-group mb-3">
                <div class="input-group-prepend">
                    <span class="input-group-text">$</span>
                </div>
                <input type="number" class="form-control" id="monthly401k">
            </div>
        </div>
        <div class="col">
            <label>Investments:</label>
            <div class="input-group mb-3">
                <div class="input-group-prepend">
                    <span class="input-group-text">$</span>
                </div>
                <input type="number" class="form-control" id="monthlyInvestments">
            </div>
        </div>
    </div>
    <div class="form-row">
        <label>Retirement:</label>
        <div class="input-group mb-3">
            <div class="input-group-prepend">
                <span class="input-group-text">$</span>
            </div>
            <input type="number" class="form-control" id="monthlyRetirement">
        </div>
    </div>
    <div class="text-center">
        <button type="button" id="monthlySavingsYearBudget" class="mx-auto btn btn-orange w-50 mt-3 unica-one">Transfer
            to Yearly
            Budget <i class="fas fa-sign-in-alt"></i>
        </button>
        <button type="button" class="mx-auto btn btn-success w-25 btn-block mt-3 unica-one monthly-submit-button">Submit
            <i class="fas fa-paper-plane"></i></button>
    </div>
</div>